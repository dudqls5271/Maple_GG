package linkUnion;

import javafx.beans.property.StringProperty;

public class linkVO {
	private StringProperty job = null;
	private StringProperty skll = null;
	private StringProperty effect = null;
	
 public linkVO() {
    }
	
	public linkVO(StringProperty job, StringProperty skll, StringProperty effect) {
		 this.job = job;
	     this.skll = skll;
	     this.effect = effect;
	}
	public String getJob() {
		return job;
	}
	public void setJob(String job) {
		this.job = job;
	}
	public String getSkll() {
		return skll;
	}
	public void setSkll(String skll) {
		this.skll = skll;
	}
	public String getEffect() {
		return effect;
	}
	public void setEffect(String effect) {
		this.effect = effect;
	}
	
	
}
