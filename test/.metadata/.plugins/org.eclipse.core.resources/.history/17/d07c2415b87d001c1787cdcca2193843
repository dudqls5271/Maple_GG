package linkUnion;

import javafx.beans.property.IntegerProperty;
import javafx.beans.property.StringProperty;

public class linkVO {
	/*
	 * private StringProperty job; private StringProperty skll; private
	 * StringProperty effect;
	 * 
	 * 
	 * public linkVO(StringProperty job, StringProperty skll, StringProperty effect)
	 * { this.job = job; this.skll = skll; this.effect = effect; } public
	 * StringProperty getJob() { return job; }
	 * 
	 * public StringProperty getSkll() { return skll; }
	 * 
	 * public StringProperty getEffect() { return effect; }
	 */

	
	private StringProperty job;
    private StringProperty skll;
    private StringProperty effect;
 
    public linkVO(StringProperty name, StringProperty address, StringProperty gender, IntegerProperty classNum) {
        this.job = job;
        this.skll = skll;
        this.effect = effect;
    }
 
    public StringProperty jobProperty() {
        return job;
    }
    public StringProperty skllProperty() {
        return skll;
    }
    public StringProperty effectProperty() {
        return effect;
    }



}
